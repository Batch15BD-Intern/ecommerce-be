{
	"kind": "collectionType",
	"collectionName": "product_items",
	"info": {
		"singularName": "product-item",
		"pluralName": "product-items",
		"displayName": "Product Item",
		"description": ""
	},
	"options": {
		"draftAndPublish": true
	},
	"pluginOptions": {},
	"attributes": {
		"price": {
			"type": "decimal"
		},
		"image": {
			"type": "media",
			"multiple": true,
			"required": false,
			"allowedTypes": ["images"]
		},
		"sku": {
			"type": "json"
		},
		"quantity": {
			"type": "integer"
		},
		"product": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::product.product",
			"inversedBy": "product_items"
		},
		"product_config": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::variation-option.variation-option",
			"inversedBy": "product_items"
		}
	}
}
