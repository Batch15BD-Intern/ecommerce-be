{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "physical_product": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "product_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-item.product-item",
      "mappedBy": "product"
    },
    "discount_codes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::discount-code.discount-code",
      "mappedBy": "products"
    }
  }
}
